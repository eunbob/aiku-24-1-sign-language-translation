image = cv2.cvtColor(image, cv2.COLOR_RGB2BGR)\n","\n","          # 손 랜드마크 그리기\n","          if hands_results.multi_hand_landmarks:\n","              for hand_landmarks in hands_results.multi_hand_landmarks:\n","                  mp_drawing.draw_landmarks(image, hand_landmarks, mp_hands.HAND_CONNECTIONS)\n","\n","          # 포즈 랜드마크 그리기\n","          if pose_results.pose_landmarks:\n","              mp_drawing.draw_landmarks(image, pose_results.pose_landmarks, mp_pose.POSE_CONNECTIONS)\n","\n","          # 결과를 출력 동영상에 쓰기\n","          out.write(image)\n","\n","          # 결과를 화면에 보여주기 (필요한 경우)\n","          # cv2.imshow('MediaPipe Hands and Pose', image)\n","          # if cv2.waitKey(1) & 0xFF == 27:\n","          #     break\n","\n","        # 자원 해제\n","        cap.release()\n","        out.release()\n","        cv2.destroyAllWindows()\n","        print(new_filename)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"h339uMI1nBsD","outputId":"e3d86b0f-8917-4397-904c-dec7d091464a","executionInfo":{"status":"ok","timestamp":1715272177514,"user_tz":-540,"elapsed":885014,"user":{"displayName":"성유진","userId":"04937647389253657125"}}},"execution_count":16,"outputs":[{"output_type":"stream","name":"stdout","text":["04_U_다리.mp4 "]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.10/dist-packages/google/protobuf/symbol_database.py:55: UserWarning: SymbolDatabase.GetPrototype() is deprecated. Please use message_factory.GetMessageClass() instead. SymbolDatabase.GetPrototype() will be removed soon.\n","  warnings.warn('SymbolDatabase.GetPrototype() is deprecated. Please '\n"]},{"output_type":"stream","name":"stdout","text":["OM_04_U_다리.mp4\n","04_L_병원.mp4 OM_04_L_병원.mp4\n","04_U_병원.mp4 OM_04_U_병원.mp4\n","04_D_병원.mp4 OM_04_D_병원.mp4\n","04_R_다리.mp4 OM_04_R_다리.mp4\n","04_R_병원.mp4 OM_04_R_병원.mp4\n","04_L_다리.mp4 OM_04_L_다리.mp4\n","04_F_병원.mp4 OM_04_F_병원.mp4\n","04_F_다리.mp4 OM_04_F_다리.mp